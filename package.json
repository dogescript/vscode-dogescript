{
    "name": "dogescript-support",
    "displayName": "dogescript-support",
    "description": "Rich dogescript language support for Visual Studio Code",
    "publisher": "dogescript",
    "author": {
        "name": "dogescript team"
    },
    "icon": "./icons/logo-tan.png",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dogescript",
                "aliases": [
                    "dogescript",
                    "dogescript"
                ],
                "extensions": [
                    ".djs"
                ],
                "configuration": "./languages/language-configuration.json",
                "icon": {
                    "dark": "./icons/logo-face-300x300.png",
                    "light": "./icons/logo-face-300x300.png"
                }
            }
        ],
        "grammars": [
            {
                "language": "dogescript",
                "scopeName": "source.djs",
                "path": "./syntaxes/dogescript.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "jest --env=node --colors test"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/jest": "^27.4.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.32.0",
        "@typescript-eslint/eslint-plugin": "^4.16.0",
        "@typescript-eslint/parser": "^4.16.0",
        "@vscode/test-electron": "^1.6.1",
        "eslint": "^7.21.0",
        "fs": "0.0.1-security",
        "glob": "^7.1.4",
        "jest": "^27.5.1",
        "nyc": "^15.1.0",
        "path": "^0.12.7",
        "source-map-support": "^0.5.12",
        "ts-jest": "^27.1.4",
        "typescript": "^4.5.5",
        "vscode-oniguruma": "^1.6.2",
        "vscode-oniguruma-wasm": "^1.1.0",
        "vscode-textmate": "^7.0.1"
    }
}